{
    "name": "node-http-exceptions",
    "description": "a top level exception handle package for nodejs",
    "license": "MIT",
    "version": "1.1.0",
    "author": "Shohel Rana",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/types/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/shohelrana-dev/node-http-exceptions"
    },
    "homepage": "https://github.com/shohelrana-dev/node-http-exceptions",
    "scripts": {
        "build": "npm run clean && npm run compile",
        "start": "node dist/esm/index.js",
        "dev": "node --watch dist/esm/index.js",
        "clean": "rm -rf dist",
        "compile": "tsc -p ./tsconfig.json && tsc -p ./tsconfig.esm.json"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "express": "^4.18.2",
        "typescript": "^5.3.3"
    },
    "keywords": [
        "exception",
        "exceptions",
        "express",
        "expressjs",
        "middleware",
        "handler",
        "http-error",
        "node-http-exceptions",
        "http-exceptions",
        "express-error"
    ]
}
